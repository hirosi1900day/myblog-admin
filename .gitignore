
/bff/.bundle
!/bff/.env*.erb

# Ignore all logfiles and tempfiles.
/bff/log/*
/bff/tmp/*
!/bff/log/.keep
!/bff/tmp/.keep

# Ignore pidfiles, but keep the directory.
/bff/tmp/pids/*
!/bff/tmp/pids/
!/bff/tmp/pids/.keep
/bff/node_modules

# Ignore storage (uploaded files in development and any SQLite databases).
/bff/storage/*
!/bff/storage/.keep
/bff/tmp/storage/*
!/bff/tmp/storage/
!/bff/tmp/storage/.keep

# Ignore master key for decrypting credentials and more.
/bff/config/master.key


